<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="39.0" layoutY="143.0" text="Input Player(s) name(s)">
            <font>
                <Font name="Impact" size="30.0" />
            </font>
        </Label>
        <TextField id="textInput" fx:id="textField" layoutX="39.0" layoutY="196.0" prefHeight="61.0" prefWidth="191.0" />
        <Label layoutX="39.0" layoutY="46.0" text="Board size">
            <font>
                <Font name="Impact" size="30.0" />
            </font>
        </Label>
        <RadioButton id="boardFour" fx:id="boardFour" layoutX="39.0" layoutY="109.0" mnemonicParsing="false" text="4">
            <toggleGroup>
                <ToggleGroup fx:id="board" />
            </toggleGroup></RadioButton>
        <RadioButton id="boardFive" fx:id="boardFive" layoutX="166.0" layoutY="109.0" mnemonicParsing="false" text="5" toggleGroup="$board" />
        <Button id="submitPlayer" fx:id="submitPlayer" layoutX="246.0" layoutY="194.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="118.0" text="SUBMIT PLAYER" />
        <Button id="submit" fx:id="submit" layoutX="246.0" layoutY="233.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="118.0" text="DONE" />
      <Label layoutX="194.0" id="error" fx:id="error" layoutY="269.0" text="Label" />
    </children>
</AnchorPane>
